{
  "name": "vue3-schema-forms-builder",
  "version": "0.4.94",
  "description": "Vue Schema Form Builder",
  "main": "dist/main.umd.js",
  "publishConfig": {
    "registry": "https://nexus3.tecna.pl/repository/npm-private-releases/"
  },
  "module": "dist/main.es.js",
  "types": "dist/components/main.d.ts",
  "type": "module",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "install-registry": "npm install --registry=https://nexus3.tecna.pl/repository/npm-private",
    "update-tecna-vue-shared": "npm install tecna-vue-shared@next --registry=https://nexus3.tecna.pl/repository/npm-private-releases/ --force",
    "dev": "echo ' to link lib run  \"npm link vue3-schema-forms-builder\" in your destination project' && npm link && vite build --watch",
    "build": "vue-tsc --noEmit && vite build",
    "build:publish": "npm run build && npm publish --registry=https://nexus3.tecna.pl/repository/npm-private-releases",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "publish:nexus": "npm publish --registry=https://nexus3.tecna.pl/repository/npm-private-releases",
    "bump-internal-libs": "npm cache clear --force && npm install tecna-vue-shared@next --registry=https://nexus3.tecna.pl/repository/npm-private-releases/ --force && npm version patch --force",
    "update-vue-forms": "npm install vue3-schema-forms@${VUE_FORMS_VERSION} --registry=https://nexus3.tecna.pl/repository/npm-private-releases/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@google/genai": "^1.13.0",
    "@mdi/font": "7.4.47",
    "@vueuse/core": "^11.1.0",
    "vue3-schema-forms": "0.4.64"
  },
  "peerDependencies": {
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "tecna-vue-shared": "^3.1.26",
    "vue": "^3.5.20",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.3",
    "vue3-toastify": "^0.2.1",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.11.4"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.1",
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/vue3-vite": "^9.1.13",
    "@types/node": "^20.14.2",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass-embedded": "^1.89.2",
    "sortablejs": "^1.15.6",
    "storybook": "^9.1.13",
    "typescript": "^5.4.5",
    "vite": "6.3.4",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-json-pretty": "^2.4.0",
    "vue-tsc": "^2.0.21",
    "vitest": "^4.0.17"
  }
}
